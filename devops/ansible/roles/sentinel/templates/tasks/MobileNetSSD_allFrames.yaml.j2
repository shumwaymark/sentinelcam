%YAML 1.0
---
# MobileNetSSD_allFrames.yaml.j2
# Review all frames in the event, log the (rects and labels) results for each.
# Template for MobileNet SSD object detection task configuration
# 
# Model paths are injected from model_registry.yaml at deployment time.
# Model versions can be overridden per-host in host_vars using model_version_overrides dict.

trk_type: obj
camwatcher_update: True

mobilenetssd: 
    confidence: 0.5
    # OpenCV DNN
    prototxt_path: {{ sentinel_models_path }}/mobilenet_ssd/{{ model_version_overrides.mobilenet_ssd | default(sentinelcam_models.mobilenet_ssd.current_version) }}/MobileNetSSD_deploy.prototxt
    model_path: {{ sentinel_models_path }}/mobilenet_ssd/{{ model_version_overrides.mobilenet_ssd | default(sentinelcam_models.mobilenet_ssd.current_version) }}/MobileNetSSD_deploy.caffemodel
    target: myriad  # [cpu, myriad]
    # Google Coral USB 
    edgetpu_model: /usr/share/edgetpu/examples/models/ssd_mobilenet_v2_coco_quant_postprocess_edgetpu.tflite
    model_labels: /usr/share/edgetpu/examples/models/coco_labels.txt
